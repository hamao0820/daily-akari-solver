{
  "manifest_version": 3,
  "name": "daily akari solver",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://dailyakari.com/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "service-worker.js"
  },
  "host_permissions": ["http://localhost:8080/*"]
}
